Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"VC", "[", 
    RowBox[{
    "t_", ",", "S_", ",", "T_", ",", "k_", ",", "a_", ",", "b_", ",", 
     "\[Sigma]_"}], "]"}], ":=", " ", 
   RowBox[{
    FractionBox["1", "2"], 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"b", 
      RowBox[{"(", 
       RowBox[{"t", "-", "T"}], ")"}]}]], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"a", 
         RowBox[{"(", 
          RowBox[{"t", "-", "T"}], ")"}]}]], "S", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Erf", "[", 
          FractionBox[
           RowBox[{
            RowBox[{"2", 
             RowBox[{"Log", "[", 
              FractionBox["S", "k"], "]"}]}], "-", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"t", "-", "T"}], ")"}], 
             RowBox[{"(", 
              RowBox[{
               SuperscriptBox["\[Sigma]", "2"], "+", 
               RowBox[{"2", "a"}]}], ")"}]}]}], 
           RowBox[{"2", 
            RowBox[{"Sqrt", "[", "2", "]"}], " ", 
            RowBox[{"Sqrt", "[", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"T", "-", "t"}], ")"}], 
              SuperscriptBox["\[Sigma]", "2"]}], "]"}]}]], "]"}], "+", "1"}], 
        ")"}]}], "+", 
      RowBox[{"k", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Erfc", "[", 
          FractionBox[
           RowBox[{
            RowBox[{"2", 
             RowBox[{"Log", "[", 
              FractionBox["S", "k"], "]"}]}], "-", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"t", "-", "T"}], ")"}], 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"2", "a"}], "-", 
               SuperscriptBox["\[Sigma]", "2"]}], ")"}]}]}], 
           RowBox[{"2", 
            RowBox[{"Sqrt", "[", "2", "]"}], 
            RowBox[{"Sqrt", "[", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"T", "-", "t"}], ")"}], 
              SuperscriptBox["\[Sigma]", "2"]}], "]"}]}]], "]"}], "-", "2"}], 
        ")"}]}]}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"VP", "[", 
   RowBox[{
   "t_", ",", "S_", ",", "T_", ",", "k_", ",", "a_", ",", "b_", ",", 
    "\[Sigma]_"}], "]"}], ":=", 
  RowBox[{
   FractionBox["1", "2"], 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"b", 
     RowBox[{"(", 
      RowBox[{"t", "-", "T"}], ")"}]}]], 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"k", " ", 
      RowBox[{"Erfc", "[", 
       FractionBox[
        RowBox[{
         RowBox[{"2", 
          RowBox[{"Log", "[", 
           FractionBox["S", "k"], "]"}]}], "-", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"t", "-", "T"}], ")"}], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "a"}], "-", 
            SuperscriptBox["\[Sigma]", "2"]}], ")"}]}]}], 
        RowBox[{"2", 
         RowBox[{"Sqrt", "[", "2", "]"}], 
         RowBox[{"Sqrt", "[", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"T", "-", "t"}], ")"}], 
           SuperscriptBox["\[Sigma]", "2"]}], "]"}]}]], "]"}]}], "-", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"a", 
        RowBox[{"(", 
         RowBox[{"t", "-", "T"}], ")"}]}]], "S", " ", 
      RowBox[{"Erfc", "[", 
       FractionBox[
        RowBox[{
         RowBox[{"2", 
          RowBox[{"Log", "[", 
           FractionBox["S", "k"], "]"}]}], "-", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"t", "-", "T"}], ")"}], 
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["\[Sigma]", "2"], "+", 
            RowBox[{"2", "a"}]}], ")"}]}]}], 
        RowBox[{"2", 
         RowBox[{"Sqrt", "[", "2", "]"}], 
         RowBox[{"Sqrt", "[", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"T", "-", "t"}], ")"}], 
           SuperscriptBox["\[Sigma]", "2"]}], "]"}]}]], "]"}]}]}], 
    ")"}]}]}]}], "Input",
